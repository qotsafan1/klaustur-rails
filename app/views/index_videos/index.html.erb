<p id="notice"><%= notice %></p>

<h1>Index Videos</h1>

<table>
  <thead>
    <tr>
      <th>Youtube</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @index_videos.each do |index_video| %>
      <tr>
        <td><%= index_video.youtube %></td>
        <td><%= link_to 'Show', index_video %></td>
        <td><%= link_to 'Edit', edit_index_video_path(index_video) %></td>
        <td><%= link_to 'Destroy', index_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Index Video', new_index_video_path %>
